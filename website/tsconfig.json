{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "types": [
      "js-cookie",
      "node"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ],
    },
  },
  "include": [
    "mock/**/*",
    "src/**/*",
    "config/**/*",
    "typings.d.ts",
    "@amap"
  ],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist",
    "typings",
    "**/__test__",
    "test",
    "docs",
    "tests"
  ],
  "files": [
    "./src/types/global.d.ts"
  ]
}